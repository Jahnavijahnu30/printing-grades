#include<stdio.h>
int main()
{
  char Grade;
  printf("Enter the marks : ");
  int marks;
  scanf("%d", &marks);
  if(marks>=85)
  { 
    printf("GradeA");
  }
  else if("marks<84 && marks>70")
  {
    printf("GradeB");
  }
  else if("marks<69 && marks>55")
  {
    printf("GradeC");
  }
  else if("marks<54 && marks>40")
  {
    printf("GradeD");
  }
  else
  {
    printf("GradeF");
  }
  return 0;
}
