#include<stdio.h>
int main()
{
	int marks;
    printf("Enter the marks : ");
    scanf("%d", &marks);
    if(marks>=85 && marks<=100)
    { 
    	printf("GradeA");
    }
  	else if(marks>=70 && marks<=84)
  	{
    	printf("GradeB");
  	}
  	else if(marks>=55 && marks<=69)
  	{
    	printf("GradeC");
  	}
  	else if(marks>=40 && marks<=54)
  	{
    	printf("GradeD");
  	}
  	else
  	{
    	printf("GradeF");
  	}
 	return 0;
}
